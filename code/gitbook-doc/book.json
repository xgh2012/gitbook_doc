{
    "title": "GitBooK使用文档",
    "author": "xgh",
    "description": "关于GitBooK自动提交发布功能",
    "language": "zh-hans",
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "anchor-navigation-ex",
	"-lunr",
	"-search",
	"search-plus",
	"code",
	"todo",
	"emphasize",
	"lightbox",
	"tbfed-pagefooter",
	"autotheme"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": true,
            "associatedWithSummary": false,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop": true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }            
        },
        "tbfed-pagefooter": {
          "copyright":"©Xgh2012",
          "modify_label": "最后修订时间：",
          "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "autotheme": {
          "white": [9, 10, 11, 12, 13, 14, 15, 16],
          "sepia": [6, 7, 8, 17, 18, 19],
          "night": [20, 21, 22, 23, 0, 1, 2, 3, 4, 5]
        }
    },
    "styles": {
        "website": "styles/website.css"
    }
}